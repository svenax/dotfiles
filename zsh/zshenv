hash -d cellar=/usr/local/Cellar
hash -d cask=/usr/local/Homebrew/Library/Taps/caskroom/homebrew-cask/Casks
hash -d vers=/usr/local/Homebrew/Library/Taps/caskroom/homebrew-versions/Casks

export GOPATH=/Users/sven/go
export GOBIN=$GOPATH/bin

path=(
    ~/bin
    $GOBIN
    ~/.composer/vendor/bin
    /Library/TeX/texbin
    /usr/local/bin
    /usr/local/sbin
    /usr/bin
    /usr/sbin
    /bin
    /sbin
)
typeset -U path
export PATH

cdpath=(
    .
    ~/src
    ~/swk
    ~/Documents/Work/
    ~/dotfiles/
)
typeset -U cdpath

fpath=(
    /usr/local/share/zsh/zsh-completions 
    /usr/local/share/zsh/functions
    $fpath
)
typeset -U fpath

