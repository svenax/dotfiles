hash -d sax=/Sources/Svenax
hash -d swk=/Sources/Work
hash -d ssax=/Sources/Work/Sven
hash -d cellar=/usr/local/Cellar
hash -d cask=/usr/local/Homebrew/Library/Taps/caskroom/homebrew-cask/Casks
hash -d vers=/usr/local/Homebrew/Library/Taps/caskroom/homebrew-versions/Casks

export QTDIR=/Users/sven/Qt5.8.0/5.8/clang_64
export GOPATH=/Sources/Go

path=(
    ~/bin
    ~/.cargo/bin
    ~/.composer/vendor/bin
    ~/.rbenv/bin
    $QTDIR/bin
    $GOPATH/bin
    /usr/local/bin
    /usr/local/sbin
    $path
)
typeset -U path
export PATH

cdpath=(
    .
    /Sources/Svenax/
    /Sources/Work/
    ~/Documents/Work/
    ~/dotfiles/
)
typeset -U cdpath

fpath=(
    /usr/local/share/zsh/zsh-completions 
    /usr/local/share/zsh/functions
    $fpath
)
typeset -U fpath

eval "$(hub alias -s)"
