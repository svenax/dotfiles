#export QTDIR=/Users/sven/Qt5.6.1/5.6/clang_64
#export QTDIR=/Users/sven/Qt5.7.1/5.7/clang_64
export QTDIR=/Users/sven/Qt5.8.0/5.8/clang_64

path=(
    ~/bin
    ~/.composer/vendor/bin
    ~/.rbenv/bin
    $QTDIR/bin
    /usr/local/bin
    /usr/local/sbin
    $path
)
typeset -U path
export PATH

cdpath=(
    .
    /Sources/Svenax/
    /Sources/Work/
    ~/Documents/Work/
    ~/dotfiles/
)
typeset -U cdpath

fpath=(
    /usr/local/share/zsh/zsh-completions 
    /usr/local/share/zsh/functions
    $fpath
)
typeset -U fpath

eval "$(rbenv init -)"
eval $(docker-machine env default)
